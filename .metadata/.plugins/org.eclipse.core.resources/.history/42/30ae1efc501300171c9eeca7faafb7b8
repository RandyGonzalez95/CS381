GfxMgr::GfxMgr()
{

}

GfxMgr::GfxMgr(Engine *eng) : Mgr(eng)
{
	mRoot = 0;
    mCamera = 0;
    mSceneMgr = 0;
    mWindow = 0;
    mResourcesCfg = Ogre::StringUtil::BLANK;
    mPluginsCfg = Ogre::StringUtil::BLANK;

    mOverlaySystem = 0;

    // OgreBites
    mCameraMan = 0;
    mDetailsPanel = 0;   	// Sample details panel
    mCursorWasVisible = false;
    mShutDown = false;
}

