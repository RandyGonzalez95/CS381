#ifndef INCLUDE_MGR_H_
#define INCLUDE_MGR_H_

class Engine;

class Mgr
{
public:
	Engine *engine;

	Mgr(Engine *eng);
	virtual ~Mgr();

	virtual void tick(float dt);
	virtual void init();
	virtual void loadLevel();
	virtual void stop();
};



#endif /* INCLUDE_MGR_H_ */
